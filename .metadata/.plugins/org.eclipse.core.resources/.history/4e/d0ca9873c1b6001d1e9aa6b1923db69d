package Lecture02_Queue;

public class Main {
	public static void main(String[] args) {
		Queue q1 = new Queue(4);
		System.out.println(q1.isEmpty());
		System.out.println(q1.isFull());
		
		q1.insert(1);
		q1.insert(2);
		q1.insert(3);
		q1.insert(4);
		
		System.out.println(q1.isEmpty());
		System.out.println(q1.isFull());
		
		while(!q1.isEmpty()) {
			System.out.println(q1.remove());
		}
		
		System.out.println(q1.isEmpty());
		System.out.println(q1.isFull());
	}
}
